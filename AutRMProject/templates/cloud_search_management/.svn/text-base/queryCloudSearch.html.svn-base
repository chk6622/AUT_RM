{% extends 'base_template/query_page/queryTable.html' %} 

{%block query_url%}/CloudSearchManagement/query{%endblock%}

{%block queryParam%}
<tr>
  <td width="98%" height="22" background="/static/tab/images/bg.gif" bgcolor="#FFFFFF" colspan='8'>
   	<div align="left">&nbsp;&nbsp;<span class="STYLE3">云搜问题查询</span></div>
  </td>
</tr>
 <tr>
 	<td width="10%" height="22" background="/static/tab/images/bg.gif" bgcolor="#FFFFFF">
   	<div align="center"><span class="STYLE1">{{queryForm.submitDate.label_tag}}</span></div>
   </td>
   <td width="15%" height="22" bgcolor="#FFFFFF" colspan='3'>
   	<div align="center"><span class="STYLE1">{{queryForm.b_submitDate}}-{{queryForm.e_submitDate}}{{queryForm.b_submitDate.help_text}}</span></div>
   </td>
   <td width="10%" height="22" background="/static/tab/images/bg.gif" bgcolor="#FFFFFF">
   	<div align="center"><span class="STYLE1">{{queryForm.finishDate.label_tag}}</span></div>
   </td>
   <td width="15%" height="22"  bgcolor="#FFFFFF" colspan='3'>
   	<div align="center"><span class="STYLE1">{{queryForm.b_finishDate}}-{{queryForm.e_finishDate}}{{queryForm.b_finishDate.help_text}}</span></div>
   </td>
 </tr>
 <tr>
	<td width="10%" height="22" background="/static/tab/images/bg.gif" bgcolor="#FFFFFF">
   	<div align="center"><span class="STYLE1">{{queryForm.problemType.label_tag}}</span></div>
   </td>
   <td width="15%" height="22" bgcolor="#FFFFFF">
   	<div align="center"><span class="STYLE1">{{queryForm.problemType}}</span></div>
   </td>
   <td width="10%" height="22" background="/static/tab/images/bg.gif" bgcolor="#FFFFFF">
   	<div align="center"><span class="STYLE1">{{queryForm.problem_qry.label_tag}}</span></div>
   </td>
   <td width="15%" height="22"  bgcolor="#FFFFFF">
   	<div align="center"><span class="STYLE1">{{queryForm.problem_qry}}</span></div>
   </td>
   <td width="10%" height="22" background="/static/tab/images/bg.gif" bgcolor="#FFFFFF">
   	<div align="center"><span class="STYLE1">{{queryForm.handleState_qry.label_tag}}</span></div>
   </td>
   <td width="15%" height="22"  bgcolor="#FFFFFF">
   	<div align="center"><span class="STYLE1">{{queryForm.handleState_qry}}</span></div>
   </td>
   <td width="10%" height="22" background="/static/tab/images/bg.gif" bgcolor="#FFFFFF">
   	<div align="center"><span class="STYLE1"></span></div>
   </td>
   <td width="15%" height="22"  bgcolor="#FFFFFF">
   	<div align="center"><span class="STYLE1"></span></div>
   </td>
 </tr>
{%endblock%}

{%block list_name%}查询云搜问题列表{% endblock%} 

{%block add_button%}
{%if perms.CloudSearchManagement.add_cloudsearch%}
<table width="90%" border="0" cellpadding="0" cellspacing="0">
	<tr>
		<td class="STYLE1"><div align="center">
				<img src="/static/tab/images/22.gif" width="14" height="14" />
			</div></td>
		<td class="STYLE1">
			<div align="center">
				<a href='/CloudSearchManagement/add/'>新增</a>
				
			</div>
		</td>
	</tr>
</table>
{%endif%}
{%endblock%}

{%block export_button%}
{%if perms.CloudSearchManagement.export_cloudsearch%}
<table width="90%" border="0" cellpadding="0" cellspacing="0">
	<tr>
		<td class="STYLE1"><div align="center">
				<img src="/static/tab/images/33.gif" width="14" height="14" />
			</div></td>
		<td class="STYLE1">
			<div align="center">
				<a href='/CloudSearchManagement/exportData/'>导出</a>
			</div>
		</td>
	</tr>
</table>
{%endif%}
{%endblock%}

{%block table_title%}
<tr>
  <td width="10%" height="22" background="/static/tab/images/bg.gif" bgcolor="#FFFFFF"><div align="center"><span class="STYLE1">提交日期</span></div></td>
  <td width="20%" height="22" background="/static/tab/images/bg.gif" bgcolor="#FFFFFF"><div align="center"><span class="STYLE1">问题类型</span></div></td>
  <td width="30%" height="22" background="/static/tab/images/bg.gif" bgcolor="#FFFFFF"><div align="center"><span class="STYLE1">问题描述</span></div></td>
  <td width="10%" height="22" background="/static/tab/images/bg.gif" bgcolor="#FFFFFF"><div align="center"><span class="STYLE1">完成日期</span></div></td>
  <td width="20%" height="22" background="/static/tab/images/bg.gif" bgcolor="#FFFFFF"><div align="center"><span class="STYLE1">处理状态</span></div></td>
  <td width="10%" height="22" background="/static/tab/images/bg.gif" bgcolor="#FFFFFF" class="STYLE1"><div align="center">操作</div></td>
</tr>
{%endblock%}

{%block table_body%}
	{% if queryResultList %}
		{%for optObj in queryResultList%}
          <tr>
          	<td height="20" bgcolor="#FFFFFF"><div align="center"><span class="STYLE1">{{optObj.submitDate|date:'Y-m-d'}}</span></div></td>
            <td height="20" bgcolor="#FFFFFF"><div align="center"><span class="STYLE1">{{optObj.get_problemType_display}}</span></div></td>     
            <td height="20" bgcolor="#FFFFFF"><div align="center"><span class="STYLE1">{{optObj.problem}}</span></div></td>
            <td height="20" bgcolor="#FFFFFF"><div align="center"><span class="STYLE1">{{optObj.finishDate|date:'Y-m-d'}}</span></div></td>
            <td height="20" bgcolor="#FFFFFF"><div align="center"><span class="STYLE1">{{optObj.get_handleState_display}}</span></div></td>
            <td height="20" bgcolor="#FFFFFF">
	            <div align="center">
		            <span class="STYLE4">
		            {%if perms.CloudSearchManagement.change_cloudsearch%}
		            <img src="/static/tab/images/edt.gif" width="16" height="16" /><a href='/CloudSearchManagement/edit/?id={{optObj.id}}'>编辑</a>
		            {%endif%}
		            {%if perms.CloudSearchManagement.delete_cloudsearch%}
		            <img src="/static/tab/images/del.gif" width="16" height="16" />
		            <a href="javascript:locationUrl('您确定删除该条记录吗？','/CloudSearchManagement/delete/?id={{optObj.id}}')">删除</a>
		            {%endif%}
		            </span>
	            </div>
            </td>
          </tr>
   		{%endfor%}
   {%endif%}
{%endblock%}

